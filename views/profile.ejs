<!DOCTYPE html>
<html lang=en>

<head>
  <%- include('./partials/head'); %>
</head>

<body>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-FKyoEForCGlyvwx9Hj09JcYn3nv7wiPVlz7YYwJrWVcXK/BmnVDxM+D2scQbITxI"
    crossorigin="anonymous"></script>
  <article class="container profile text-center position_relative">
    <div class="mx-auto text-start bg-white shadow-lg">
      <header class="pt-4 pt-md-0 profile-header">
        <div class="row">
          <div class="col-block col-md-auto text-center text-md-start">
            <img class="profile-pic" src="images/picture.jpg" alt="">
          </div><!--//col-->
          <div class="col">
            <div class="row p-4 justify-content-center justify-content-md-between">
              <div class="primary-info col-auto">
                <h1 class="name mt-0 mb-1 text-uppercase text-uppercase">
                  <%= first_name %>
                    <%= last_name %>
                </h1>
                <div class="title mb-3">Backend Developer</div>
                <ul class="list-unstyled">
                  <li class="mb-2"><a class="text-link" href="#"><i class="far fa-envelope fa-fw me-2"
                        data-fa-transform="grow-3"></i>
                      <%= email_address %>
                    </a></li>
                  
                  <% if(phone_number) { %>
                    <li><a class="text-link" href="#"><i class="fas fa-mobile-alt fa-fw me-2"
                        data-fa-transform="grow-6"></i>
                      <%= phone_number %>
                      </a></li>
                  <% }%>
                </ul>
              </div><!--//primary-info-->
              <div class="secondary-info col-auto mt-2">
                <ul class="list-unstyled">
                  <li class="mb-3"><a class="text-link" href="https://www.github.com/codolv"><span class="fa-container text-center me-2"><i
                          class="fa-brands fa-github-alt fa-fw"></i></span>GitHub</a></li>
                  <li class="mb-3"><a class="text-link" href="https://www.github.com/codolv"><span class="fa-container text-center me-2"><i
                          class="fa-brands fa-linkedin fa-fw"></i></span>LinkedIn</a></li>
                  <li class="mb-3"><a class="text-link" href="https://www.github.com/codolv"><span class="fa-container text-center me-2"><i
                          class="fa-brands fa-xing fa-fw"></i></span>Xing</a></li>
                  <li class="mb-3"><a class="text-link" href="https://www.github.com/codolv"><span class="fa-container text-center me-2"><i
                          class="fas fa-globe"></i></span>Freelancemap</a></li>
                </ul>
              </div><!--//secondary-info-->
            </div><!--//row-->
          </div>
        </div>
      </header>
      <div class="body p-5">
        <section class="mb-5">
          <h2 class="section-title text-uppercase font-weight-bold pb-3 mb-3">Ãœber mich</h2>
          <div class="section-content">
            <p class="mb-0">
              <%= description %>
            </p>
          </div>
        </section>
        <div class="row">
          <div class="col-lg-9">
            <section class="mb-5">
              <h2 class="section-title text-uppercase font-weight-bold pb-3 mb-3">Projekte</h2>
              <div class="section-content">
                <div class="timeline position-relative">
                  <% for(var i=0; i < projects.length; i++) { %>
                    <article class="timeline-item position-relative pb-5">
                      <div class="timeline-item-header mb-2">
                        <div class="d-flex flex-column flex-md-row">
                          <h3 class="project-name font-weight-bold mb-1">
                            <%= projects[i].name %>
                          </h3>
                          <div class="role-name ms-auto">
                            <%= projects[i].role %>
                          </div>
                        </div><!--//row-->
                        <div class="position-time">
                          <%= projects[i].from_date.toLocaleDateString("de-DE") %> bis <%=
                              projects[i].till_date.toLocaleDateString("de-DE") %>
                        </div>
                      </div><!--//resume-timeline-item-header-->
                      <div class="timeline-item-desc">
                        <p>
                          <%= projects[i].description %>
                        </p>
                        <h4 class="timeline-item-desc-heading font-weight-bold">Benutzte Technologien:</h4>
                        <ul class="list-inline list-online-horizontal">
                          <% for(var j=0; j < projects[i].techs.length; j++) { %>
                            <li class="list-inline-item"><span class="badge p-badge badge-pill">
                                <%= projects[i].techs[j] %>
                              </span></li>
                            <% } %>
                        </ul>
                      </div><!--//resume-timeline-item-desc-->
                    </article><!--//resume-timeline-item-->
                    <% } %>
                </div>
              </div>
            </section>
          </div>
          <div class="col-lg-3">
            <section class="mb-5">
              <h2 class="section-title text-uppercase font-weight-bold pb-3 mb-3">Erfahrungen</h2>
              <div class="section-content">
                <div class="skill-item">
                  <h4 class="skills-cat font-weight-bold">Programmiersprachen</h4>
                  <ul class="list-unstyled mb-4">
                    <% for(var i = 0; i < skills.length; i++) { %>
                      <% if(skills[i].category=="Programmiersprache" ) { %>
                        <li class="mb-2">
                          <div class="skill-name">
                            <%= skills[i].name %>
                          </div>
                          <div class="progress profile-progress">
                            <div class="progress-bar theme-progress-bar-dark" role="progressbar" style="width: <%= skills[i].scale * 10 %>%"
                              aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                          </div>
                        </li>
                        <% } } %>
                  </ul>
                </div>
                <div class="skill-item">
                  <h4 class="skills-cat font-weight-bold">Frameworks</h4>
                  <ul class="list-unstyled mb-4">
                    <% for(var i=0; i < skills.length; i++) { %>
                      <% if(skills[i].category=="Framework" ) { %>
                        <li class="mb-2">
                          <div class="skill-name">
                            <%= skills[i].name %>
                          </div>
                          <div class="progress profile-progress">
                            <div class="progress-bar theme-progress-bar-dark" role="progressbar" style="width: <%= skills[i].scale * 10 %>%"
                              aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                          </div>
                        </li>
                        <% } } %>
                  </ul>
                </div>
                <div class="skill-item">
                  <h4 class="skills-cat font-weight-bold">Tools</h4>
                  <ul class="list-unstyled mb-4">
                    <% for(var i=0; i < skills.length; i++) { %>
                      <% if(skills[i].category=="Tools" ) { %>
                        <li class="mb-2">
                          <div class="skill-name">
                            <%= skills[i].name %>
                          </div>
                          <div class="progress profile-progress">
                            <div class="progress-bar theme-progress-bar-dark" role="progressbar" style="width: <%= skills[i].scale * 10 %>%"
                              aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                          </div>
                        </li>
                        <% } } %>
                  </ul>
                </div>
              </div>
            </section>
            <section class="mb-5">
              <h2 class="section-title text-uppercase font-weight-bold pb-3 mb-3">Sprachen</h2>
              <div class="section-content">
                <div>
                  <ul class="list-unstyled mb-4">
                    <% for(var i=0; i < skills.length; i++) { %>
                      <% if(skills[i].category=="Sprache" ) { %>
                        <li class="mb-2">
                          <div>
                            <%= skills[i].name %>
                          </div>
                          <div class="progress profile-progress">
                            <div class="progress-bar theme-progress-bar-dark" role="progressbar" style="width: <%= skills[i].scale * 10 %>%"
                              aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                          </div>
                        </li>
                        <% }} %>
                  </ul>
                </div>
              </div>
            </section>
          </div>
        </div>
      </div>
    </div>
    </div>
  </article>
</body>
<html>